definition {

	macro addProductVersionInBuild {
		Click(locator1 = "Button#PLUS");

		Type(
			locator1 = "TestrayBuild#TYPE_PRODUCT_VERSION_IN_BUILD",
			value1 = ${key_name});

		Click(locator1 = "TestrayManage#SAVE_BUTTON_IN_MODAL");
	}

	macro assertTitleModal {
		AssertElementNotPresent(
			key_title = ${key_itemvalue},
			locator1 = "TestrayManage#TITLE_MODAL_ITEM");
	}

	macro changeTeamToComponent {
		TestrayManage.selectTableRowOption(
			rowOption = "Edit",
			tableEntry = ${tableEntry});

		Select(
			key_fieldLabel = "Team",
			locator1 = "Select#GENERIC_SELECT_FIELD",
			value1 = ${newTeamName});

		Button.clickSave();
	}

	macro closeModalInOptions {
		Click(locator1 = "TestrayManage#CLOSE_MODAL");
	}

	macro createCaseTypes {
		Click(locator1 = "TestrayManage#ADD_PROJECT_ON_MODAL");

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = ${caseName});

		Click(locator1 = "TestrayManage#SAVE_BUTTON");
	}

	macro createComponent {
		SelectFrame(locator1 = "Testray#MODAL_BODY");

		Click(locator1 = "TestrayManage#ADD_PROJECT_ON_MODAL");

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = ${componentName});

		if (isSet(teamName)) {
			Select(
				key_fieldLabel = "Team",
				locator1 = "Select#GENERIC_SELECT_FIELD",
				value1 = ${teamName});
		}

		Button.clickSave();
	}

	macro createNewCategory {
		Click(locator1 = "TestrayManage#ADD_PROJECT_ON_MODAL");

		if (isSet(category)) {
			Type(
				locator1 = "TestrayManage#NEW_OPTION_NAME",
				value1 = ${category});
		}

		Click(locator1 = "TestrayManage#SAVE_BUTTON");
	}

	macro createNewOption {
		Click(locator1 = "TestrayManage#ADD_PROJECT_ON_MODAL");

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = ${option});

		Select(
			locator1 = "TestrayManage#SELECT_CATEGORY_IN_OPTIONS",
			value1 = ${category});

		Click(locator1 = "TestrayManage#SAVE_BUTTON");
	}

	macro createProductVersion {
		Click(locator1 = "TestrayManage#ADD_PROJECT_ON_MODAL");

		if (isSet(productVersion)) {
			Type(
				locator1 = "TestrayManage#NEW_OPTION_NAME",
				value1 = ${productVersion});
		}

		Click(locator1 = "TestrayManage#SAVE_BUTTON");
	}

	macro createTeam {
		Click(locator1 = "TestrayManage#ADD_PROJECT_ON_MODAL");

		if (isSet(teamName)) {
			Type(
				locator1 = "TestrayManage#NEW_OPTION_NAME",
				value1 = ${teamName});
		}

		if (isSet(component)) {
			Click(
				key_parameterType = "Unassigned",
				locator1 = "TestraySuites#SMART_SUITE_PARAMETER_TYPE",
				value1 = ${component});

			Click(
				key_arrowDirection = "Right",
				key_parameterType = "Unassigned",
				locator1 = "TestraySuites#SMART_SUITE_ARROW");
		}

		Button.clickSave();
	}

	macro deleteAllCasesTypes {
		Refresh();

		TestrayDropdown.selectOptionSidebar(
			buttonOption = "Manage",
			menuItem = "Case Types");

		Testray.changePagination(
			modal = "true",
			quantity = 150);

		while (IsElementPresent(key_tableEntry = "Case_", locator1 = "Testray#TABLE_ENTRY")) {
			TestrayManage.selectTableRowOption(
				rowOption = "Delete",
				tableEntry = "Case_");

			AssertConfirm(value1 = "Are you sure you want to delete this item?");

			Alert.viewSuccessMessage();

			Testray.checkSuccessAlertAppearsAndDisappears();
		}
	}

	macro deleteAllCategories {
		Refresh();

		TestrayDropdown.selectOptionSidebar(
			buttonOption = "Manage",
			menuItem = "Categories");

		while (IsElementPresent(key_tableEntry = "Environments_", locator1 = "Testray#TABLE_ENTRY")) {
			TestrayManage.selectTableRowOption(
				rowOption = "Delete",
				tableEntry = "Environments_");

			AssertConfirm(value1 = "Are you sure you want to delete this item?");

			Alert.viewSuccessMessage();

			Testray.checkSuccessAlertAppearsAndDisappears();
		}
	}

	macro deleteCasesTypes {
		TestrayManage.selectTableRowOption(
			rowOption = "Delete",
			tableEntry = ${tableEntry});

		AssertConfirm(value1 = "Are you sure you want to delete this item?");

		Alert.viewSuccessMessage();

		Testray.checkSuccessAlertAppearsAndDisappears();
	}

	macro deleteCategory {
		TestrayDropdown.selectOptionSidebar(
			buttonOption = "Manage",
			menuItem = "Categories");

		TestrayManage.selectTableRowOption(
			rowOption = "Delete",
			tableEntry = ${categoryName});

		AssertConfirm(value1 = "Are you sure you want to delete this item?");

		Alert.viewSuccessMessage();

		Testray.checkSuccessAlertAppearsAndDisappears();
	}

	macro editCaseType {
		TestrayManage.selectTableRowOption(
			rowOption = "Edit",
			tableEntry = ${tableEntry});

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = ${newName});

		Click(locator1 = "TestrayManage#SAVE_BUTTON");

		Testray.checkSuccessAlertAppearsAndDisappears();
	}

	macro editCategory {
		TestrayManage.selectTableRowOption(
			rowOption = "Edit",
			tableEntry = ${tableEntry});

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = ${newName});

		Click(locator1 = "TestrayManage#SAVE_BUTTON");
	}

	macro editComponent {
		TestrayManage.selectTableRowOption(
			rowOption = "Edit",
			tableEntry = ${tableEntry});

		if (isSet(newComponentName)) {
			Type(
				locator1 = "TestrayManage#NEW_OPTION_NAME",
				value1 = ${newComponentName});
		}

		if (isSet(newTeamName)) {
			Select(
				key_fieldLabel = "Team",
				locator1 = "Select#GENERIC_SELECT_FIELD",
				value1 = ${newTeamName});
		}

		Button.clickSave();
	}

	macro editOption {
		TestrayManage.selectTableRowOption(
			rowOption = "Edit",
			tableEntry = ${tableEntry});

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = ${newName});

		Select(
			locator1 = "TestrayManage#SELECT_CATEGORY_IN_OPTIONS",
			value1 = ${category});

		Click(locator1 = "TestrayManage#SAVE_BUTTON");

		Testray.checkSuccessAlertAppearsAndDisappears();
	}

	macro editProductVersion {
		TestrayManage.selectTableRowOption(
			rowOption = "Edit",
			tableEntry = ${tableEntry});

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = ${newName});

		Click(locator1 = "TestrayManage#SAVE_BUTTON");
	}

	macro editTeam {
		TestrayManage.selectTableRowOption(
			rowOption = "Edit",
			tableEntry = ${tableEntry});

		Type(
			locator1 = "TestrayManage#NEW_OPTION_NAME",
			value1 = ${newName});

		Click(locator1 = "TestrayManage#SAVE_BUTTON");
	}

	macro selectOptionDropdownCategory {
		Click(locator1 = "TestrayManage#DROPDOWN_CATEGORIES");

		MenuItem.clickNoError(menuItem = ${menuItem});

		Alert.viewSuccessMessage();
	}

	macro selectTableRowOption {
		WaitForElementPresent(
			key_tableEntry = ${tableEntry},
			locator1 = "Testray#TABLE_ENTRY");

		RightClick.rightClick(
			key_tableEntry = ${tableEntry},
			locator1 = "Testray#TABLE_ENTRY");

		MenuItem.clickNoError(menuItem = ${rowOption});
	}

}